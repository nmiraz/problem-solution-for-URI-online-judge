#include<iostream>
  using namespace std;
  int main()
  {
      int a,b,c,d,res;
      cin >>a>>b>>c>>d;
      res = ((a*b)-(c*d));
      cout << "DIFERENCA = "<< res<<endl;
      return 0;
  }
  
  
