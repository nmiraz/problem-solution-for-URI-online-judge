#include<iostream>
  using namespace std;
   int main()
   {
       int a,b;
       float c,res;
       cin >> a>>b>>c;
       res=b*c;
       cout <<"NUMBER = "<<a<<endl;
       cout.setf(ios::fixed,ios::floatfield);
       cout.precision(2);
       cout << "SALARY = U$ "<<res<<endl;
       return 0;


   }
   
   
   
