#include<iostream>
  using namespace std;
  int main()
  {
      double a,b,c,resa,resb,resc,resd,rese;
      cin >>a>>b>>c;
      resa=(0.5*a*c);
      resb=3.14159*c*c;
      resc=0.5*(a+b)*c;
      resd=b*b;
      rese=a*b;
      cout.setf(ios::fixed,ios::floatfield);
      cout.precision(3);
      cout << "TRIANGULO: "<<resa<<endl<<"CIRCULO: "<<resb<<endl<<"TRAPEZIO: "<<resc<<endl<<"QUADRADO: "<<resd<<endl<<"RETANGULO: "<<rese<<endl;
      return 0;
  }
  
  
  
