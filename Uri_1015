#include<iostream>
#include<cmath>
  using namespace std;
  int main()
  {
      double x1,y1,x2,y2,p,res;
      cin >>x1>>y1>>x2>>y2;
      p=((x2-x1)*(x2-x1))+((y2-y1)*(y2-y1));
      res = sqrt(p);
      cout.setf(ios::fixed,ios::floatfield);
      cout.precision(4);
      cout << res<<endl;
      return 0;
  }
  
  
  
