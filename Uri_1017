#include<iostream>
 using namespace std;
 int main()
 {
     double t,s,res;
     cin >>t>>s;
     res = (t*s)/12;
     cout.setf(ios::fixed,ios::floatfield);
     cout.precision(3);
     cout << res<<endl;
     return 0;
 }
 
 
 
