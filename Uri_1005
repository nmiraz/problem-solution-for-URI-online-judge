#include<iostream>
   using namespace std;
    int main()
    {
       float a,b,c,MEDIA;
       cin >> a>>b;
       MEDIA= (a*3.5+b*7.5)/(3.5+7.5);
       cout.setf(ios::fixed,ios::floatfield);
       cout.precision(5);
       cout << "MEDIA"<<" = "<<MEDIA<<endl;
       return 0;
    }



