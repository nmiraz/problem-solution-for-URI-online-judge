#include<iostream>
 using namespace std;
 int main()
 {
    double d,f,res;
    cin >> d>>f;
    res = d/f;
    cout.setf(ios::fixed,ios::floatfield);
    cout.precision(3);
    cout <<res<<" km/l"<<endl;
    return 0;
 }
 
 
 
