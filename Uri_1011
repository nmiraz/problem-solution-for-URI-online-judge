#include<iostream>
 using namespace std;
   int main()
   {
       double r,res;
       cin >>r;
       double m=r*r*r;
       res = (1.333333333*3.14159)*m;
       cout.setf(ios::fixed,ios::floatfield);
       cout.precision(3);
       cout << "VOLUME = "<<res<<endl;
       return 0;
   }
   
   
   
